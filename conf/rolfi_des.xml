	<extractionUnits>
		<extUnit id="lookupMILE32" connectionId="remotePostgres"
			stopOnError="true" active="true"  frequencyInSeconds="20" insertBatchSize="500" >
			<sql>select a.numero, a.presventa, a.grcomision, a.tipo,
					a.existencia, a.fecultvta,
					a.cantultcpa, a.fecmod, i.fecemision, i.descuento, c.cliente, c.vendedor, c.transporte, c.condventa, c.nropedido
				from articulo a, itcabeza c, ititems i
				where a.numero = i.articulo and i.nrocomp = c.nrocomp 
				and i.fecemision is not null
				and c.nropedido is not null 
				</sql>
			<description>lookupMILE</description>
			<columns>
			<column sourceColumn="numero" triggerUpdate="true" isPKMember="true"> </column>
			<column sourceColumn="fecemision" triggerUpdate="true" isPKMember="true"> </column>
			<column sourceColumn="cliente" triggerUpdate="true" isPKMember="true"> </column>
			<column sourceColumn="nropedido" triggerUpdate="true" isPKMember="true"> </column>
			</columns>
		</extUnit>
